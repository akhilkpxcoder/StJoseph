
@{
    ViewBag.Title = "User Approval";
    Layout = "~/Views/_ViewAdministrator.cshtml";
}

<!DOCTYPE html>
<html>
<head>

   
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body>

    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Role</th>
                    <th>Approval</th>
                    <th>Action</th>
                </tr>
            </thead>
            <br />
            <tbody>

                @for (int i = 0; i < Model.Rows.Count; i++)
                {

                    <tr>
                        <td>@Model.Rows[i][1]</td>
                        <td>@Model.Rows[i][6]</td>
                        <td>@Model.Rows[i][10]</td>
                        <td>
                            <a href="@Url.Action("EditApproval","Administrator",new{@id=@Model.Rows[i][0] })">Edit</a>
                        </td>
                    </tr>
                }

            </tbody>
        </table>

    </div>
</body>
</html>
